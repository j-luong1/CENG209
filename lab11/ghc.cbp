<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ghc" />
		<Option makefile="makefile" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="ghc" prefix_auto="1" extension_auto="1" />
				<Option object_output="/media/tech153/labs/ghc/.objs" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<MakeCommands>
					<Build command="$make -f $makefile" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile" />
					<SilentBuild command="$make -f $makefile &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="ghcontrol.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="ghcontrol.h" />
		<Unit filename="ghglgmain.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="ghglgmain.h" />
		<Unit filename="ghmain.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="ghutils.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="ghutils.h" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
